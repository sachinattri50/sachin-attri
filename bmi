def BMI(weight, height):
    weight=int(input())
    height=float(input())
    # to convert height from cm to m2
    height=height/10000
    bmi=weight/float(height**2)
    
    return bmi
bmi=BMI(weight,height)
print(bmi)
if (bmi<18.5):
    print("underweight")
elif(bmi>=18.5 and bmi<24.9):
    print ("healthy")
elif(bmi>=24.9 and bmi<30):
    print(overweight)
else:
    print("obesity")
